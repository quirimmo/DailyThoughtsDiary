{
	"expo": {
		"name": "Daily Thoughts Diary",
		"description": "An app for managing daily thoughts with a diary for keeping track of them",
		"slug": "daily-thoughts-diary",
		"privacy": "public",
		"sdkVersion": "30.0.0",
		"platforms": ["ios", "android"],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"android": {
			"package": "com.quirimmo.dailythoughtsdiary"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.quirimmo.dailythoughtsdiary",
			"supportsTablet": true
		}
	}
}
